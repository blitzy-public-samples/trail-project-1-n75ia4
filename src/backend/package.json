{
  "name": "task-management-backend",
  "version": "1.0.0",
  "description": "Enterprise-grade backend service for the Task Management System",
  "main": "dist/app.js",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "tsc -p tsconfig.json",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "docker:build": "docker build -t task-management-backend .",
    "security:audit": "npm audit",
    "validate:types": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/lib-storage": "^3.0.0",
    "@nestjs/cache-manager": "^2.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/swagger": "^10.0.0",
    "@prisma/client": "^5.0.0",
    "@sendgrid/mail": "^7.7.0",
    "amqplib": "^0.10.3",
    "argon2": "^0.31.0",
    "bcryptjs": "^2.4.3",
    "bottleneck": "^2.0.0",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "clamav.js": "^1.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-cache-middleware": "^2.0.0",
    "express-correlation-id": "^2.0.1",
    "express-openapi-validator": "^5.0.1",
    "express-rate-limit": "^6.0.0",
    "express-status-monitor": "^1.3.4",
    "helmet": "^8.0.0",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "ioredis": "^5.0.0",
    "jsonwebtoken": "^9.0.0",
    "opossum": "^6.0.0",
    "prom-client": "^14.2.0",
    "rate-limiter-flexible": "^3.0.0",
    "redis": "^4.6.0",
    "retry": "^0.13.1",
    "sanitize-html": "^2.11.0",
    "socket.io": "^4.6.0",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "winston": "^3.8.0",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-elasticsearch": "^0.17.4",
    "ws": "^8.0.0",
    "ws-rate-limit": "^1.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@jest/globals": "^29.0.0",
    "@jest/types": "^29.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-prettier": "^4.2.0",
    "ioredis-mock": "^8.0.0",
    "jest": "^29.0.0",
    "jest-mock-extended": "^3.0.0",
    "nock": "^13.3.3",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}